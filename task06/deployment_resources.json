{
  "lambda_path": "/Users/mac/aws-tasks/task06",
  "func_name": "aws.task06.AuditProducer",
  "lambda-basic-execution": {
    "policy_content": {
      "Statement": [
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents",
            "dynamodb:GetItem",
            "dynamodb:Query",
            "dynamodb:PutItem",
            "dynamodb:UpdateItem",
            "dynamodb:Batch*",
            "dynamodb:DeleteItem",
            "dynamodb:GetRecords",
            "dynamodb:GetShardIterator",
            "dynamodb:DescribeStream",
            "dynamodb:ListStreams",
            "ssm:PutParameter",
            "ssm:GetParameter",
            "kms:Decrypt",
            "lambda:InvokeFunction"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "resource_type": "iam_policy"
  },
  "audit_producer-role": {
    "predefined_policies": [
      "AmazonDynamoDBFullAccess"
    ],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution"
    ],
    "resource_type": "iam_role",
    "allowed_accounts": [
      "${account_id}"
    ]
  },
  "Configuration": {
    "resource_type": "dynamodb_table",
    "hash_key_name": "key",
    "hash_key_type": "S",
    "read_capacity": 1,
    "write_capacity": 1,
    "global_indexes": [],
    "autoscaling": [],
    "Configuration-stream": {
      "table": "Configuration",
      "resource_type": "dynamodb_stream",
      "stream_view_type": "NEW_AND_OLD_IMAGES"
    }
  },
  "Audit": {
    "resource_type": "dynamodb_table",
    "hash_key_name": "itemKey",
    "hash_key_type": "S",
    "read_capacity": 1,
    "write_capacity": 1,
    "global_indexes": [],
    "autoscaling": []
  }
}